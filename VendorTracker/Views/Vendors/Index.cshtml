@using VendorTracker.Models;

<h1>Vendor Roll Call:</h1>
@if (@Model.Count == 0)
{
    <p>You currently do not have any vendors on your list.</p>;
}

@foreach(Vendor vendor in Model)
{
  <h2><a href="/vendors/@vendor.Id">@vendor.Name</a></h2>  
}


<a href="/vendors/new">Fill out new vendor form!</a><br>
<a href="/">Return to Home</a>